{
  "name": "blockmine",
  "version": "1.5.3",
  "description": "Мощная панель управления ботами для Майнкрафта.",
  "author": "merka",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blockmineJS/blockmine.git"
  },
  "bin": {
    "blockmine": "backend/cli.js"
  },
  "keywords": [
    "minecraft",
    "bot",
    "mineflayer",
    "dashboard",
    "panel",
    "automation"
  ],
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=backend\" \"npm run dev --workspace=frontend\"",
    "postinstall": "npm install --workspace=frontend --ignore-scripts && prisma generate --schema=./backend/prisma/schema.prisma",
    "build": "npm run build --workspace=frontend",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "@radix-ui/react-icons": "^1.3.2",
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "cmdk": "^1.1.1",
    "cron-parser": "^5.3.0",
    "express": "^4.19.2",
    "express-validator": "^7.2.1",
    "framer-motion": "^12.19.2",
    "fs-extra": "^11.3.0",
    "iconv-lite": "^0.6.3",
    "jsonwebtoken": "^9.0.2",
    "mineflayer": "^4.20.1",
    "multer": "^2.0.1",
    "node-cron": "^4.1.0",
    "pidusage": "^3.0.2",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "prisma": "^5.14.0",
    "react-textarea-autosize": "^8.5.9",
    "react-virtualized-auto-sizer": "^1.0.26",
    "semver": "^7.6.2",
    "socket.io": "^4.7.5",
    "socks": "^2.8.5",
    "sonner": "^2.0.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
